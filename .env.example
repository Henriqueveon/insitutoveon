# =====================================================
# CONFIGURAÇÃO DO CLOUDFLARE R2
# Copie este arquivo para .env e preencha com suas credenciais
# =====================================================

# Credenciais do Cloudflare R2
R2_ACCOUNT_ID=seu_account_id
R2_ACCESS_KEY_ID=sua_access_key
R2_SECRET_ACCESS_KEY=sua_secret_key
R2_ENDPOINT=https://seu_account_id.r2.cloudflarestorage.com
R2_BUCKET_NAME=nome_do_bucket
R2_PUBLIC_URL=https://seu-dominio-publico.r2.dev

# Variáveis do Vite (frontend)
VITE_R2_PUBLIC_URL=https://seu-dominio-publico.r2.dev
VITE_R2_BUCKET_NAME=nome_do_bucket
